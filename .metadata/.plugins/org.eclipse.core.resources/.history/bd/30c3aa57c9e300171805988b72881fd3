package cn.cas.cnic.log.fileread;

import java.util.HashMap;
import java.util.Vector;

public class SecureRead extends FileRead{
	public SecureRead(String FileName) {
		super(FileName);
	}
	
	@Override  //对于secure日志，必须经等于号的处理和多出的逗号的处理
	protected Vector<String> breakdown(String str) {
		Vector<String> result = new Vector<String>();
		String[] strs = str.split(" ");
		StringBuilder sb = new StringBuilder();
		for (String s : strs) {
			if(s.endsWith(",") || s.endsWith(":"))
				s = s.substring(0, s.length()-1);
			String[] strArray = s.split("=");
			if(strArray.length == 2) {
				sb.setLength(0);
				sb.append(strArray[0]).append("=");
				result.add(sb.toString());   
				result.add(strArray[1]);
			}else {
				result.add(s);
			}
		}
		return result;
	}
	
	@Override
	protected HashMap<segmentInformation, String> dealLogByLine(String LogLine) {
		// TODO Auto-generated method stub
		return null;
	}
}
